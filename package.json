    "apollo": "^2.31.1",
    "apollo-link": "^1.2.14",
    "eslint-plugin-prettier": "^3.1.4",
    "formik": "^2.1.5",
    "graphql": "^15.4.0",
    "lodash": "^4.17.20",
    "logkitty": "^0.7.1",
    "node-fetch": "^2.6.1",
    "node-notifier": "^9.0.0",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-config": "^1.4.1",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-reanimated": "^2.10.0",
